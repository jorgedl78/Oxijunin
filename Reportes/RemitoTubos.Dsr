Private Sub DataReport_Initialize()
    Set rs = cn.Execute("select Fecha, Puesto, Numero, Movimiento  from RemitosTubos inner join EstadoTubos on RemitosTubos.idEstadoTubos=EstadoTubos.idEstadoTubos where idRemitoTubos=" & idRemitoTubo)
    With RemitoTubos.Sections("Sección4")
        .Controls("lblNumero").Caption = Format(rs!Puesto, "0000") & "-" & Format(rs!numero, "00000000")
        .Controls("lblFecha").Caption = rs!Fecha
        .Controls("lblEstado").Caption = rs!Movimiento
    End With
    
