    '    .Controls("lblSaldoFinal").Caption = Format(rs!final, "0.00")
    'End With
        
    Set rs = cn.Execute("SELECT Tubos.Numero, Articulos.Descripcion, Tubos.Capacidad, UnidadesMedidas.Unidad FROM DetalleRemitoTubos INNER JOIN Tubos ON DetalleRemitoTubos.idTubo = Tubos.idTubo INNER JOIN Articulos ON Tubos.idArticulo = Articulos.idArticulo INNER JOIN UnidadesMedidas ON Tubos.idUnidadMedida = UnidadesMedidas.idUnidadMedida AND Tubos.idUnidadMedida = UnidadesMedidas.idUnidadMedida AND Tubos.idUnidadMedida = UnidadesMedidas.idUnidadMedida AND Tubos.idUnidadMedida = UnidadesMedidas.idUnidadMedida And Tubos.idUnidadMedida = UnidadesMedidas.idUnidadMedida Where (DetalleRemitoTubos.idRemitoTubos = " & idRemitoTubo & ") order by Descripcion")
    Set RemitoTubos.DataSource = rs
End Sub
